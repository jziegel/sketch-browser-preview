{
  "name": "Browser Preview",
  "identifier": "com.sketchapp.browserpreview",
  "version": "1.0.0",
  "description": "Generates a preview of your current artboard and shows in your web browser.",
  "authorEmail": "jcebxd@gmail.com",
  "author": "Jul Ceballos",
  "manifestUrl": "https://github.com/Freakland/sketch-browser-preview/blob/master/BrowserPreview.sketchplugin/Contents/Sketch/manifest.json",
  "downloadUrl": "https://github.com/Freakland/sketch-browser-preview/archive/master.zip",
  "compatibleVersion": "4.1",
  "commands": [
    {
      "script": "preview.js",
      "handler": "mobilePreview",
      "shortcut": "cmd shift .",
      "name": "Mobile Device",
      "identifier": "mobile_preview"
    },
    {
      "script": "preview.js",
      "handler": "desktopPreview",
      "shortcut": "cmd shift .",
      "name": "Desktop Device",
      "identifier": "desktop_preview"
    }
  ],
  "menu": {
    "title" : "Preview on Browser",
    "items" : [
      "mobile_preview",
      "desktop_preview"
    ]
  }
}